{
  "name": "chatgpt-discord-bot",
  "version": "2.0.0",
  "type": "module",
  "description": "The ultimate AI-powered Discord bot",
  "main": "dist/index.js",
  "scripts": {
    "build": "bun run lint && swc --delete-dir-on-start src --out-dir dist",
    "build:all": "bun run build && bun run check:types",
    "check:types": "bunx tsc --noEmit",
    "lint": "bunx biome format --write ./src/",
    "start:gateway": "bun run build && node ./dist/gateway/index.js",
    "start:db": "bun run build && node ./dist/db/index.js",
    "start:bot": "bun run build && node ./dist/bot/index.js",
    "git": "git fetch && git pull"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TuringAI-Team/chatgpt-discord-bot.git"
  },
  "keywords": [
    "chatgpt"
  ],
  "author": "TuringAI",
  "license": "CDDL-1.0",
  "bugs": {
    "url": "https://github.com/TuringAI-Team/chatgpt-discord-bot/issues"
  },
  "homepage": "https://github.com/TuringAI-Team/chatgpt-discord-bot#readme",
  "devDependencies": {
    "@biomejs/biome": "1.1.2",
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@discordeno/bot": "^19.0.0-next.a087a52",
    "@discordeno/gateway": "^19.0.0-next.a087a52",
    "@discordeno/rest": "^19.0.0-next.a087a52",
    "@discordeno/utils": "^19.0.0-next.a087a52",
    "@supabase/supabase-js": "^2.33.2",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.85",
    "axios": "^1.5.0",
    "bun-types": "^1.0.2",
    "express": "^4.18.2",
    "rabbitmq-client": "^4.3.0",
    "redis": "^4.6.8",
    "turing.sh": "^0.0.6"
  }
}
